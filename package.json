{
	"name": "semantic-highliting",
	"description": "Does semantic highlight like Kdevelop and pycharm",
	"version": "0.0.1",
	"publisher": "MalcolmMielle",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"repository": {
		"url": "https://gitlab.com/MalcolmMielle/semantic-highliting.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "myextension.largeNumberBackground",
				"description": "Background decoration color for large numbers",
				"defaults": {
					"dark": "#FF000055",
					"light": "#FF000055",
					"highContrast": "#FF000055"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"@types/vscode": "^1.32.0"
	},
	"keywords": [
		"python",
		"semantic",
		"highlight"
	],
	"dependencies": {
		"vsce": "^1.100.1"
	}
}
