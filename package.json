{
	"name": "semantic-highlighting",
	"description": "Does semantic highlight like Kdevelop and pycharm",
	"version": "0.4.0",
	"publisher": "MalcolmMielle",
	"license": "MIT",
	"icon": "images/coding.png",
	"displayName": "Semantic highlighting",
	"engines": {
		"vscode": "^1.32.0"
	},
	"repository": {
		"url": "https://gitlab.com/MalcolmMielle/semantic-highlighting.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"@types/vscode": "^1.32.0"
	},
	"keywords": [
		"python",
		"semantic",
		"highlight",
		"pycharm",
		"kdevelop",
		"rainbow"
	],
	"dependencies": {
		"vsce": "^1.100.1"
	}
}
